<h1>Games#new</h1>
<p>Find me in app/views/games/new.html.erb</p>
<p><%= @letters.join %></p>
<form action="score" method="post">
<%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <input type="text" name="word" placeholder="Best word here">
  <input type="submit">
  <input type="hidden" name="letters" value="<%=@letters%>">
</form>
